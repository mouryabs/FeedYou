<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sandeshs.feedyou"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sandeshs.feedyou.SecondPage"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sandeshs.feedyou.DONreg"
            android:label="Give your food here" />
        <activity
            android:name="com.example.sandeshs.feedyou.NGOreg"
            android:label="Registration" />
        <activity
            android:name="com.example.sandeshs.feedyou.ListPage"
            android:label="Available Donors" />
        <activity
            android:name="com.example.sandeshs.feedyou.FirstPage"
            android:label="@string/title_activity_first_page" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sandeshs.feedyou.Confirm"
            android:label="@string/title_activity_confirm"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>